<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Dashboard V.1 | jeweler - Material Admin Template</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- favicon
        ============================================ -->
    <link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/img/favicon.ico")">
    <!-- Google Fonts
        ============================================ -->
    <link href="https://fonts.googleapis.com/css?family=Play:400,700" rel="stylesheet">
    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/font-awesome")
    @Styles.Render("~/Content/owl")
    @Styles.Render("~/Content/animate")
    @Styles.Render("~/Content/normalize")
    @Styles.Render("~/Content/meanmenu")
    @Styles.Render("~/Content/main")
    @Styles.Render("~/Content/morris")
    @Styles.Render("~/Content/mCustomScrollbar")
    @Styles.Render("~/Content/metisMenu")
    @Styles.Render("~/Content/fullcalendar")
    @Styles.Render("~/Content/style")
    @Styles.Render("~/Content/responsive")
    @Scripts.Render("~/Scripts/modernizr")
</head>
<body>
    <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
    @Html.Partial("LeftSidebar")
    <!-- Start Welcome area -->
    <div class="all-content-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="logo-pro">
                        <a href="index.html"><img class="main-logo" src="@Url.Content("~/img/logo/logo.png")" )" alt="" /></a>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-advance-area">
            @Html.Partial("HeaderTop")
            @Html.Partial("MobileMenu")
            @Html.Partial("BreadcomeArea")
        </div>
        <div class="container-fluid">
            @RenderBody()
        </div>
        @Html.Partial("Footer")
    </div>
    @Scripts.Render("~/Scripts/jquery")
    @Scripts.Render("~/Scripts/bootstrap")
    @Scripts.Render("~/Scripts/wow")
    @Scripts.Render("~/Scripts/jquery.price.slider")
    @Scripts.Render("~/Scripts/meanmenu")
    @Scripts.Render("~/Scripts/owl.carousel")
    @Scripts.Render("~/Scripts/jquery.sticky")
    @Scripts.Render("~/Scripts/jquery.scrollUp")
    @Scripts.Render("~/Scripts/mCustomScrollbar")
    @Scripts.Render("~/Scripts/metisMenu")
    @Scripts.Render("~/Scripts/morris")
    @Scripts.Render("~/Scripts/sparkline")
    @Scripts.Render("~/Scripts/fullcalendar")
    @Scripts.Render("~/Scripts/plugins")
    @Scripts.Render("~/Scripts/main")
    @RenderSection("Scripts", required: false)
    <script type="text/javascript">
        @RenderSection("ScriptsReady", required: false)
    </script>
</body>
</html>