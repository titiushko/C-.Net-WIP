        function findSWF(movieName) {
            if (navigator.appName.indexOf("Microsoft") != -1) {
                return window[movieName];
            } else {
                return document[movieName];
            }
        }

        function reloadChart(chartName, chartUrl) {
            var movieUrl = "";
            if (chartUrl == "Contratos por Mes") {
                movieUrl = "./modules/data_contractspermonth.php?UserBank=88";
            } else if (chartUrl == "Contratos por Banco") {
                movieUrl = "./modules/data_contractsperbanks.php?UserBank=88";
            } else if (chartUrl == "Autos") {
                movieUrl = "./modules/data_carsperbrands.php?UserBank=88";
            } else if (chartUrl == "Productividad por Usuario") {
                movieUrl = "./modules/data_contractsperuser.php?UserBank=88";
            }
            tmp = findSWF(chartName);
            x = tmp.reload(movieUrl);
        }

        function ChangeCharts() {
            var DateStart = $("#DateStartC").val();
            var DateEnd = $("#DateEndC").val();
            switch ($("#Graph").val()) {
                case "Contratos":
                    var movieUrl = "./modules/data_contractspermonth.php?UserBank=88";
                    var moviel = "contracts";
                    break;
                case "Bancos":
                    var movieUrl = "./modules/data_contractsperbanks.php?UserBank=88";
                    var moviel = "banks";
                    break;
                case "Autos":
                    var movieUrl = "./modules/data_carsperbrands.php?UserBank=88";
                    var moviel = "assets";
                    break;
                case "Productividad":
                    var movieUrl = "./modules/data_contractsperuser.php?UserBank=88";
                    var moviel = "productivity";
                    break;
            }
            whereclause = "DateStart=" + DateStart + "&DateEnd=" + DateEnd;
            tmp = findSWF(moviel);
            x = tmp.reload(movieUrl + "&" + whereclause);
