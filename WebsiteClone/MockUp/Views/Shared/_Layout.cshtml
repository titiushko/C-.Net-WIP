    <script type="text/javascript">
        var BASE_URL = "@(Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/') + "/")";
        var updateClock = false;
        var runClock = function (run) {
            updateClock = run;
            ticktock.run();
        };
        (function (w, d) {
            var clock = document.querySelector('digiclock');
            var zeroPrefix = function (x) {
                return x < 10 ? '0' + x : x;
            };
            w.ticktock = {
                run: function () {
                    var d = new Date();
                    var h = zeroPrefix(d.getHours());
                    var m = zeroPrefix(d.getMinutes());
                    var s = zeroPrefix(d.getSeconds());
                    if (updateClock) {
                        clock.innerHTML = [h, m, s].join(':');
                        setInterval(ticktock.run, 1000);
                    }
                }
            };
            ticktock.run();
        })(window, document);
    </script>
